<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox alignment="CENTER_LEFT" spacing="5.0">
         <children>
            <Button fx:id="btnHire" mnemonicParsing="false" onAction="#onHireNewEmployees" text="Hire" />
            <Separator orientation="VERTICAL" />
            <Label text="HR Capacity: " />
            <Label fx:id="lblCapa" text="CAPA" />
            <Separator orientation="VERTICAL" />
            <Label text="Used: " />
            <Label fx:id="lblUsed" text="USED" />
            <Pane prefHeight="10.0" prefWidth="10.0" HBox.hgrow="ALWAYS" />
         </children>
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </HBox>
      <ListView fx:id="listOverview" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
         <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </VBox.margin>
      </ListView>
   </children>
</VBox>
